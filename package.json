{
  "name": "muzup-workers",
  "version": "1.0.0",
  "description": "Worker processor for muzup queues",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "rm -rf ./dist && tsc && pnpm copy-files",
    "build:watch": "tsc-watch --onSuccess \"pnpm start:dev\"",
    "start:dev": "pnpm copy-files && pnpm start",
